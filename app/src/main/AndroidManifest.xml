<?xml version="1.0" encoding="utf-8"?>
<!-- suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iuvo.iuvo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".ProfileSetup"
            android:label="@string/profile_setup" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/study_session" >
        </activity>
        <activity
            android:name=".NewEvent"
            android:label="@string/title_activity_new_event" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.iuvo.iuvo.MainActivity" />
        </activity>
    </application>

</manifest>
